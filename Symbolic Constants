#include <stdio.h>
#include <cs50.h>

#define LOWER 0
#define UPPER 300
#define STEP 20

//rather than set lower = 0, I have defined a string to be such as LOWER 0//


int main(void)
{
    printf("Fahrenheit-Celsius Table\n");
    
        int fahr;
        
        for (fahr = LOWER; fahr <= UPPER; fahr = fahr + STEP)
            printf("%3d %6.1f\n", fahr, (5.0/9.0)*(fahr - 32));
}
